extends layout

block content
    nav
        a.home(href="/home") Home
        a.yourActivities(href="/useractivity") Your Activities
        a.logout(href="/logout") Logout
    
    div.everything
        h2.List List of Activities I am tracking
        h4 Activity ID
        ul
            each activity in activitiesDones
                a(href=`/useractivity/${activity.ActivityId}`)
                    li= activity.ActivityId
                